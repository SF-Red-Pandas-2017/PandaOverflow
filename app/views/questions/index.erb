<br><br>
<h1>Questions</h1>

<ul>
<% @questions.each do |question| %>
  <li><a href="questions/<%= question.id %>"><%= question.title %></a></li>
  <span>votes: <%= question.object_votes %></span>
  <span>answers: <%= question.total_answers %></span>
  <br><br>
<% end %>
</ul>

<div class="add_question">
  <a href="questions/new">Add question</a>
</div>
